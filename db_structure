create table user (username VARCHAR(100), password VARCHAR(100) , id INT AUTO_INCREMENT , PRIMARY KEY (id));
create table uploads (userid INT, filename VARCHAR(100) , id INT AUTO_INCREMENT ,orgfilename VARCHAR(100), PRIMARY KEY (id));
create table questions (id INT AUTO_INCREMENT, userid INT, question VARCHAR(100),PRIMARY KEY(id));
create table tagmap (questionid INT, tagid INT);
create table tags (id INT AUTO_INCREMENT, tag VARCHAR(100), info VARCHAR(200),PRIMARY KEY(id));